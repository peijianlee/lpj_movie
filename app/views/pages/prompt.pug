doctype
html
	head
		meta(charset="utf-8")
		title #{message}
		meta(name="robots" content="nofollow")
		link(href="/favicon.ico", type="image/x-icon", rel="shortcut icon")
		link(href="/css/error.css",rel="stylesheet")

	body.bg-gray
		.bg-box
		.wrap-box
			a.logo(href="/", title="返回首页")
				img(src="/images/logo-index.png", alt="")
			.info-box-stand
			.infoBox
				.info
					i !
					p #{message}
				.infoBtn
					a.button.primary(href="/")
						span 返回首页
					a.button.action(href="/store")
						span 查看商品


